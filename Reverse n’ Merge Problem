#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
     int n,i;
    scanf("%d",&n);
    for(i=1;i<n;i++)
    {
    printf("%d",i);
     printf(" ");
    }
    for(i=n;i>0;i--)
    {
    printf("%d",i);
    printf(" ");
    }
    return 0;
}
